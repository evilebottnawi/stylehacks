{
  "name": "stylehacks",
  "version": "1.0.0",
  "description": "Detect/remove browser hacks from CSS files.",
  "main": "dist/index.js",
  "bin": {
    "stylehacks": "bin/cmd.js"
  },
  "files": [
    "bin",
    "LICENSE-MIT",
    "dist"
  ],
  "scripts": {
    "prepublish": "babel src --out-dir dist --ignore /__tests__/",
    "test-unformatted": "babel-tape-runner \"src/**/__tests__/*.js\"",
    "test": "npm run test-unformatted | tap-spec"
  },
  "keywords": [
    "browsers",
    "css",
    "hack",
    "hacks",
    "optimise",
    "postcss",
    "postcss-plugin",
    "stylehacks"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-tape-runner": "^1.2.0",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0"
  },
  "homepage": "https://github.com/ben-eb/stylehacks",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/stylehacks",
  "dependencies": {
    "browserslist": "^1.0.0",
    "chalk": "^1.1.1",
    "log-symbols": "^1.0.2",
    "minimist": "^1.2.0",
    "plur": "^2.0.0",
    "postcss": "^5.0.4",
    "postcss-reporter": "^1.1.0",
    "postcss-selector-parser": "^1.2.0",
    "read-file-stdin": "^0.2.0",
    "text-table": "^0.2.0",
    "write-file-stdout": "0.0.2"
  }
}
