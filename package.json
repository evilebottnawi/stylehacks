{
  "name": "stylehacks",
  "version": "2.2.0",
  "description": "Detect/remove browser hacks from CSS files.",
  "main": "dist/index.js",
  "bin": {
    "stylehacks": "dist/cli.js"
  },
  "files": [
    "LICENSE-MIT",
    "dist",
    "usage.txt"
  ],
  "scripts": {
    "pretest": "eslint src",
    "prepublish": "del-cli dist && BABEL_ENV=publish babel src --out-dir dist --ignore /__tests__/",
    "test": "npm run prepublish && ava src/__tests__"
  },
  "keywords": [
    "browsers",
    "css",
    "hack",
    "hacks",
    "optimise",
    "postcss",
    "postcss-plugin",
    "stylehacks"
  ],
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "del-cli": "^0.2.0",
    "eslint": "^2.2.0",
    "eslint-config-cssnano": "^2.0.0"
  },
  "homepage": "https://github.com/ben-eb/stylehacks",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/stylehacks",
  "dependencies": {
    "browserslist": "^1.0.0",
    "chalk": "^1.1.1",
    "log-symbols": "^1.0.2",
    "minimist": "^1.2.0",
    "plur": "^2.0.0",
    "postcss": "^5.0.5",
    "postcss-reporter": "^1.2.1",
    "postcss-selector-parser": "^1.2.0",
    "read-file-stdin": "^0.2.0",
    "text-table": "^0.2.0",
    "write-file-stdout": "0.0.2"
  },
  "eslintConfig": {
    "extends": "cssnano"
  },
  "ava": {
    "require": "babel-core/register"
  }
}
